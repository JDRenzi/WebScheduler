<div id="search_mobile_bar" style="display:none;">

  <form method="get" action="/search" id="main_searchbox_mobile" class="clearfix cf">
    <input type="hidden" name="from" value="">
    <div class="right">
      <input id="search-mobile-button" type="button" class="button button-padding special-button left" value="GO">
    </div>
    <div style="overflow:hidden;height:30px;">
      <input name="q" id="search_mobile_input" placeholder="Anime series, drama, etc" value="" type="text" class="left placeholder" autocomplete="off"/>
    </div>
  </form>
  <script type="text/javascript">

    $("#search_mobile_input").val($("#search_mobile_input").attr("placeholder"));
    $("#search_mobile_input").focus(function(){
      focus_text($("#search_mobile_input"), $("#search_mobile_input").attr("placeholder"));
      ComboSearch.preload();
      ComboSearch.search("header_search_input");
    });

    $("#search_mobile_input").blur(function() {
      blur_text($("#search_mobile_input"), $("#search_mobile_input").attr("placeholder"));
      if(!$('.search-autocomplete-mobile-outer').is(':hover')) {
        $('.search-autocomplete-mobile-outer').hide();
      }
    });

    // $('html').on('click touchstart', function() {
    //   if($('.search-autocomplete-mobile-outer').is(':visible')) {
    //     $('.search-autocomplete-mobile-outer').hide();
    //   }
    // });

    // $('#header_search_autocomplete_mobile').on('click touchstart', function(event){
    //   event.stopPropagation();
    // });

    $("#search_mobile_input").keyup(function(e) {
      ComboSearch.search("search_mobile_input");
      if(e.keyCode == 27){
        ComboSearch.close();
        $('.search-autocomplete-mobile-outer').hide();
        $(this).val('');
      }
      $('#header_search_autocomplete_mobile').html($('#header_search_autocomplete').html());
      if ($('#header_search_autocomplete_mobile a').length > 0) {
        $('.search-autocomplete-mobile-outer').show();
      } else {
        $('.search-autocomplete-mobile-outer').hide();
      }
    });
    $("#search_mobile_input").keydown(ComboSearch.handleKeyPress);

    $(document).click(ComboSearch.handleDocumentClick);

    $("#search-mobile-button").click(function() {
      if($("#search_mobile_input").val()==$("#search_mobile_input").attr("placeholder")) {
        return false;
      }
      $("#search_mobile_input").closest("form").submit();
    });
  </script>
  <div class="search-autocomplete-mobile-outer" style="display: none;">
    <div id="header_search_autocomplete_mobile" class="search-autocomplete-mobile"> </div>
  </div>
  <div style="clear:both;"></div>
</div>
<script type="text/javascript">
  //Mobile nav
  $('html').on('click touchstart', function() {
    if($('#dropdown_mobile').is(':visible')) {
      $('#dropdown_mobile').removeClass('dropdown-mobile-visible');
      $('#header_home').removeClass('header-home-shadow');
      $('.mini-menu').removeClass('mini-menu-active');
    }
  });

  $('#header_mobilemenu_beta, #dropdown_mobile').on('click touchstart', function(event){
    event.stopPropagation();
  });
  //// tool tip
  // hide or show
  var thisLi;
  $("#header_userpanel_beta li").mouseover(function(){
    thisLi = $(this);
    showHeaderTooltip(thisLi, thisLi.find(".tooltip"));
    thisLi.find("a").mousedown(function(){
      hideTooltip(thisLi, thisLi.find(".tooltip"));
    });
  });
  $("#header_userpanel_beta li").mouseout(function(){
    hideTooltip(thisLi, thisLi.find(".tooltip"));
  });
  function hideTooltip(parent, tooltip){
    clearTimeout(parent.data('timeout'));
    tooltip.stop(true, true).animate({opacity:0}, 200, function(){
      $(this).css({'visibility' : 'hidden'});
    });
  }
  function showHeaderTooltip(parent, tooltip){
    var tooltipDesc = tooltip.find(".tooltip-desc");
    alignTooltip(tooltipDesc);
    parent.data('timeout', setTimeout(function(){
      tooltip.css({'visibility' : 'visible', 'opacity' : 0}).animate({opacity:1}, 200);
    }, 450));
  }

  // align tool tip
  function alignTooltip(tooltipDesc){
    var tooltipWidth;
    tooltipWidth = tooltipDesc.outerWidth();
    tooltipMargin = -tooltipWidth / 2;
    tooltipDesc.parent().css({'left' : '50%', 'margin-left' : tooltipMargin, 'width' : Math.max(tooltipWidth, 52)});
  }

  // set width for arrows (ie9 fix)
  function setWidth(divHasWidth, divNoWidth){
    var divWithWidth;
    divWithWidth = divHasWidth.outerWidth();
    divNoWidth.css({'width' : divWithWidth});
  }

  //// end of tool tip

  //// user drop down
  var userMenu = $("#header_userpanel_beta .user-icon");
  userMenu.click(function(event){
    dropdownToggle($(this));
    userMenu.toggleClass("open");
    event.stopPropagation();
  });

  var userDropdown = $(".dropdown");
  function dropdownToggle(sibling){
    sibling.next(userDropdown).toggle();
    setWidth(userDropdown.find('.dropdown-menu'), userDropdown.find('.tooltip-top'));
  }
  $(userDropdown).click(function(event){
    event.stopPropagation();
  });

  $('html').click(function(){
    $(".dropdown").hide();
    userMenu.removeClass("open");
  });
  //// end of drop down

  /* Set window resize behavior*/
  function resizeEnd(callback) {
    var rtime = new Date(1, 1, 2000, 12,00,00);
    var timeout = false;
    var delta = 200;
    $(window).resize(function() {
      rtime = new Date();
      if (timeout === false) {
        timeout = true;
        setTimeout(resizeend, delta);
      }
    });

    function resizeend() {
      if (new Date() - rtime < delta) {
        setTimeout(resizeend, delta);
      } else {
        timeout = false;
        callback();
      }
    }
  }
  /*resizeEnd(function() {
    if($(window).width() <= 985) {
      $('.search').remove().insertAfter('#header_beta');
    } else {
      $('#header_searchpanel_beta').append($('.search').remove());
    }
  });*/

</script>

